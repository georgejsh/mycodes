package euler;

public class p43substdiv {
	static int a[];
	public static boolean findnext(){
		int i=9;
		while(!set(i)){
			i--;
			if(i<0)
				return false;
		}
		return true;
	}
	public static boolean set(int i){
		if(a[i]==9)
			return false;
		a[i]++;
		int b[]=new int[10];
		for(int j=0;j<=i;j++)
			b[a[i]]++;
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
