package euler;

import java.util.Vector;

public class p23absum {
	public static int countdiv(int n){
		int i,j=0;
		for(i=1;i<n;i++)
			if(n%i==0)
				j+=i;
		return j;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Vector<Integer> li=new Vector<>();
		Vector<Integer> l=new Vector<>();
		int i,j=0,k;
		for(i=1;i<28123;i++){
			k=countdiv(i);
			if(k>i){
				li.addElement(i);
			}
		}
		for(i=1;i<28123;i++){
			l.add(0);
		}
			
		
	}

}
